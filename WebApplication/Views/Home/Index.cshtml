                @{ var prod = productTable.FirstOrDefault();
                    if (prod != null) {
                    <div class="sales">
                        <div class="tag">New</div>
                        <div class="off">
                            <div class="discount">
                                <h2>10</h2>
                                <div class="text">
                                    <h3 class="h-47 bold">%</h3>
                                    <h4 class="h-27 bold">OFF</h4>
                                </div>
                            </div>
                            <h1 class="h-61 bold light-black mb-32">@prod.ProductName</h1>
                            <a href="@Url.Action("ProductDetail", "Home", new { productId = prod.ProductId })" class="cus-btn sec">Purchase Now</a>
                        </div>
                        <div class="product-img">
                            <img src="@prod.ImagePath" alt="">
                        </div>
                    </div>
                        }
                    }
            </div>
        </div>
    </section>
